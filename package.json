{
  "name": "dark-docs-monorepo",
  "version": "2.0.0",
  "description": "Dark Docs 2.0 - Chrome extension and landing page",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "clean": "turbo clean && rm -rf node_modules",
    "build:extension": "turbo build --filter=extension",
    "build:landing": "turbo build --filter=landing",
    "dev:extension": "turbo dev --filter=extension",
    "dev:landing": "turbo dev --filter=landing"
  },
  "keywords": [
    "chrome-extension",
    "nextjs",
    "google-docs",
    "dark-mode",
    "theme",
    "browser-extension",
    "landing-page"
  ],
  "author": "Ameya Lambat",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^2.3.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.2.1"
}
