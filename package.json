{
  "name": "dark-docs-monorepo",
  "version": "2.0.2",
  "description": "Dark Docs 2.0 - Chrome extension and landing page",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ameyalambat128/dark-docs-2.0.git"
  },
  "homepage": "https://darkdocs.ameyalambat.com",
  "bugs": {
    "url": "https://github.com/ameyalambat128/dark-docs-2.0/issues"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "format:check": "turbo format --check",
    "clean": "turbo clean && rm -rf node_modules",
    "build:extension": "turbo build --filter=extension",
    "build:www": "turbo build --filter=www",
    "dev:extension": "turbo dev --filter=extension",
    "dev:www": "turbo dev --filter=www"
  },
  "keywords": [
    "chrome-extension",
    "nextjs",
    "google-docs",
    "dark-mode",
    "theme",
    "browser-extension",
    "landing-page",
    "firefox-extension",
    "edge-extension",
    "opera-extension",
    "manifest-v3",
    "turborepo",
    "monorepo"
  ],
  "author": "Ameya Lambat",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^2.3.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.2.1"
}
